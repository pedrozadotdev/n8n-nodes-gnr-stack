{
  "name": "n8n-nodes-http-forward-auth",
  "version": "0.1.0",
  "description": "This is an n8n community node. It can be used as a HTTP forward auth middleware with reverse proxies like Traefik and Caddy.",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "AndrÃ© Pedroza",
    "email": "andre@pedroza.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pedrozadotdev/n8n-nodes-http-forward-auth.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "build-docker": "pnpm i && pnpm build && chown -R node:node ./dist",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "test": "jest --silent=false",
    "test-watch": "jest --watch"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/HttpForwardAuth/HttpForwardAuth.node.js",
      "dist/nodes/HttpForwardAuth/HttpForwardAuthTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@scure/base": "^1.2.4",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "4.0.0-beta1",
    "prettier": "^3.3.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "redis": "^4.7.0"
  }
}
